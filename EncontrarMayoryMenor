using namespace std;

void main ()
{
	int mayor,menor, numero1, numero2, numero3, numero4, numero5;
cout<<"ingresa numero 1 : ";
cin>>numero1;
cout<<"ingresa numero 2 : ";
cin>>numero2;
cout<<"ingresa numero 3 :";
cin>>numero3;
cout<<"ingresa numero 4: ";
cin>>numero4;
cout<<"ingresa numero 5 : ";
cin>>numero5;
mayor=numero1;
menor=numero1;

	if (mayor < numero1)
		mayor=numero1;
	if (menor>numero1)
		menor=numero1;
	if (mayor < numero2)
		mayor=numero2;
	if (menor>numero2)
		menor=numero2;
	if (mayor<numero3)
		mayor=numero3;
	if (menor>numero3)
		menor=numero3;
	if (mayor<numero4)
		mayor=numero4;
	if (menor>numero4)
		menor=numero4;
	if (mayor<=numero5)
		mayor=numero5;

	cout<<"El numero mayor es: "<<mayor<<endl;
	cout<<"El numero menor es: "<<menor<<endl;

 getch();

